    "concurrently": "^3.5.0",
    "element-ui": "^1.4.7",
    "eslint": "^4.9.0",
    "koa": "^2.0.0-alpha.8",
    "koa-logger": "^3.1.0",
    "koa-mysql": "^1.0.3",
    "koa-router": "^7.2.1",
    "koa-static": "^4.0.1",
    "node-sass": "^4.5.3",
    "request": "^2.83.0",
    "vue": "^2.4.4",
    "vue-router": "^3.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
