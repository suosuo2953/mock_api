{
  "name": "mock_api",
  "description": "A Vue.js project",
  "version": "1.0.0",
  "author": "wrq",
  "private": true,
  "scripts": {
    "start": "concurrently \"cross-env NODE_ENV=development webpack-dev-server --open --hot\" \"nodemon ./src/api/ --exec babel-node\"",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
    "api": "nodemon ./src/api/ --exec babel-node",
    "buildapi": "babel ./src/api/ -d dist",
    "serve": "node dist/index.js",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules"
  },
  "dependencies": {
    "babel-core": "^6.26.0",
